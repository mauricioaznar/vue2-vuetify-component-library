{
  "name": "maguas-vuetify2",
  "version": "0.1.74",
  "private": false,
  "scripts": {
    "dev": "vue-cli-service serve dev/dev.ts",
    "build": "vue-cli-service build --target lib --name maguas-vuetify2 src/main.ts",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "pub": "git add . && git commit -m \"Updates\" && npm version patch && npm run build && npm publish --access public"
  },
  "main": "./dist/maguas-vuetify2.umd.js",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.0.3",
    "axios": "^0.20.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.2.1",
    "sass": "^1.32.0",
    "sass-loader": "^10.0.0",
    "typescript": "~4.1.5",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.12",
    "vue-cli-plugin-vuetify": "~2.4.0",
    "vue-router": "^3.5.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.4.11",
    "vuetify-loader": "^1.7.0"
  },
  "peerDependencies": {
    "vue": "2.x",
    "vuetify": "2.4.x",
    "vee-validate": "3.x",
    "axios": "0.x"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "@typescript-eslint/no-var-requires": 0,
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/consistent-type-assertions": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel"
  }
}
